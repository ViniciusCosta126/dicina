{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/accordion-item",
	"title": "Item do acordeão",
	"description": "Componente item do acordeão",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-accordion-item"],
	"render": "file:./render.php",
	"attributes": {
		"title": {
			"type": "string",
			"default": "superacao"
		},
