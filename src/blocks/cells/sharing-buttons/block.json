{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/sharing-buttons",
	"title": "Botões de compartilhamento",
	"description": "Botões de compartilhamento para mídias sociais",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": ["file:./style.scss", "style-post-footer"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"enables": {
			"type": "array",
			"default": ["facebook", "linkedin", "twitter", "whatsapp", "copy"]
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"bgColor": "#ffffff"
		}
	}
}
