{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/gallery-mosaic",
	"title": "Galeria em mosaico",
	"description": "Galeria com carrossel de mosaicos",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-gallery-mosaic"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"galleryID": {
			"type": "string",
			"default": "gallery-mosaic"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"currentFocus": {
			"type": "string",
			"default": "mosaic-item"
		},
		"initialLoad": {
			"type": "boolean",
			"default": true
		},
		"itemsLength": {
			"type": "integer",
			"default": 0
		},
		"mosaicCounter": {
			"type": "integer",
			"default": 1
		},
		"teste": {
			"type": "array",
			"default": [0]
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#002753"
		}
	}
}
