{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/gallery-carousel",
	"title": "Gallery Carousel",
	"description": "Galeria de imagemn em carrossel",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-gallery-carousel"],
	"render": "file:./render.php",
	"attributes": {
		"appender": {
			"type": "boolean",
			"default": true
		},
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"id": {
			"type": "string",
			"default": "gallery-carousel"
		},
		"itemsLength": {
			"type": "integer",
			"default": 0
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#002753"
		}
	}
}
