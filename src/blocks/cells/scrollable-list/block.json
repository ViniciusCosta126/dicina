{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/scrollable-list",
	"title": "Lista Scrolável",
	"description": "Lista de links scrolável",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-scrollable-list"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "details"
		},
		"bgColor": {
			"type": "string",
			"default": "transparent"
		},
		"itemsLength": {
			"type": "integer",
			"default": 0
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"initialLoad": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "transparent"
		}
	}
}
