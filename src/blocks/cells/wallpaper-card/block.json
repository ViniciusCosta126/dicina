{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/wallpaper-card",
	"title": "Card wallpaper",
	"description": "Componente para card de wallpaper",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-wallpaper-card"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "transparent"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"modalId": {
			"type": "string",
			"default": "modal-id"
		},
		"thumbImg": {
			"type": "object",
			"default": {
				"src": "https://placehold.co/384x384",
				"alt": "Imagem placeholder",
				"width": "720",
				"height": "960"
			}
		},
		"wallpaper": {
			"type": "object",
			"default": {
				"src": "https://placehold.co/720x960",
				"alt": "Imagem placeholder",
				"width": "720",
				"height": "960"
			}
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "transparent"
		}
	}
}
