{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/newslletter-sennaticos",
	"title": "Newslletter sennaticos",
	"description": "Componente para newslletter de sennaticos",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-newslletter-sennaticos"],
	"render": "file:./render.php",
	"attributes": {
		"title": {
			"type": "string",
			"default": "Faça"
		},
		"textoPrincipal": {
			"type": "object",
			"default": {
				"normal": "Fique por dentro do"
			}
		},
		"titleMobile": {
			"type": "object",
			"default": {
				"normal": "Fique parte da nossa comunidade de fãs e fique por dentro das novidades"
			}
		},
		"titleDesktop": {
			"type": "object",
			"default": {
				"normal": "Fique parte da nossa comunidade de fãs e fique por dentro das novidades"
			}
		},
		"inputPlaceholder": {
			"type": "string",
			"default": "Digite seu e-mail"
		},
		"buttonLabel": {
			"type": "string",
			"default": "Inscreva-se agora"
		},
		"bgImage": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/newsletter-bg-sennaticos.jpg"
			}
		},
		"bgColor": {
			"type": "string",
			"default": "linear-gradient(90deg, rgba(0, 168, 81, 0.7) 0%, rgba(24, 50, 215, 0.7) 100%)"
		},
		"variant": {
			"type": "string",
			"default": "default"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"errorMessage": {
			"type": "string",
			"default": "Erro ao enviar, tente novamente."
		},
		"successMessage": {
			"type": "string",
			"default": "Inscrição realizada!"
		},
		"requiredMessage": {
			"type": "string",
			"default": "*Campo Obrigatório"
		},
		"isTemplated": {
			"type": "boolean",
			"default": false
		},
		"titleEn": {
			"type": "string",
			"default": "Faça"
		},
		"textoPrincipalEn": {
			"type": "object",
			"default": {
				"normal": "Fique por dentro do"
			}
		},
		"titleMobileEn": {
			"type": "object",
			"default": {
				"normal": "Be part of the group of"
			}
		},
		"titleDesktopEn": {
			"type": "object",
			"default": {
				"normal": "Be part of our"
			}
		},
		"errorMessageEn": {
			"type": "string",
			"default": "Error sending, please try again."
		},
		"successMessageEn": {
			"type": "string",
			"default": "Enrollment completed"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "linear-gradient(90deg, rgba(0, 168, 81, 0.7) 0%, rgba(24, 50, 215, 0.7) 100%)"
		}
	}
}
