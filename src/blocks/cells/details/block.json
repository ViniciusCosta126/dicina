{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/details",
	"title": "Detalhes",
	"description": "Bloco para apresentação de detalhes",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-details"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "transparent"
		},
		"hasIcon": {
			"type": "boolean",
			"default": true
		},
		"hasNumber": {
			"type": "boolean",
			"default": true
		},
		"hasText": {
			"type": "boolean",
			"default": true
		},
		"hasTextLabel": {
			"type": "boolean",
			"default": false
		},
		"id": {
			"type": "string",
			"default": ""
		},
		"isActive": {
			"type": "boolean",
			"default": true
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "transparent"
		}
	}
}
