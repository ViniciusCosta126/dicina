{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/animated-senna",
	"title": "Máscara Senna",
	"description": "Seção animada a partir do scroll com máscara e imagem do senna",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": ["file:./style.scss", "style-home-section-2"],
	"render": "file:./render.php",
	"attributes": {
		"title1": {
			"type": "string",
			"default": "Ayrton"
		},
		"title2": {
			"type": "string",
			"default": "Senna"
		},
		"imgMobile": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/olhar-senna-mobile.jpg"
			}
		},
		"imgTablet": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/olhar-senna-tablet.jpg"
			}
		},
		"imgDesk": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/olhar-senna-desk.jpg"
			}
		},
		"img4k": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/olhar-senna-4k.jpg"
			}
		}
	}
}
