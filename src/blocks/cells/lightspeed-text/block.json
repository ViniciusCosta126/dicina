{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/lightspeed-text",
	"title": "Lightspeed Text",
	"description": "Se o de texto com anima o lightspeed",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"viewScript": "file:view.js",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-lightspeed-text"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"containerSize": {
			"type": "string",
			"default": "container default"
		},
		"mobileVisible": {
			"type": "boolean",
			"default": true
		},
		"gspStart": {
			"type": "string",
			"default": "center-=250 center"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}
