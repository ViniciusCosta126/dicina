{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/contact-forms",
	"title": "Contact-Forms",
	"description": "",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": [
		"file:./style.scss",
		"style-contact-forms"
	],
	"render": "file:./render.php",
	"attributes": {
		"title": {
			"type": "string",
			"default": "Faça"
		},
		"textoPrincipal": {
			"type": "object",
			"default": {
				"normal": "nós queremos",
				"sans-serif": "te ouvir!"
			}
		},
		"subtitle": {
			"type": "string",
			"default": "Envie para o nosso time sua mensagem de apoio ou sugestão preenchendo os campos ao lado."
		},
		"bgImage": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/contact-bg.jpg"
			}
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"errorMessage": {
			"type": "string",
			"default": "Erro ao enviar, tente novamente."
		},
		"successMessage": {
			"type": "string",
			"default": "Inscrição realizada!"
		},
		"requiredMessage": {
			"type": "string",
			"default": "*Campo Obrigatório"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}