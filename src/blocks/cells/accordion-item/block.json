		"contentAlign":{
			"type": "string",
			"default": "right"
		},
		"imgMobile": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/mao-mobile.jpeg"
			}
		},
		"imgTablet": {
			"type": "object",
			"default": {
				"id": "",
				"src": ""
			}
		},
		"imgDesk": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/mao-desk.jpeg"
			}
		},
		"img4k": {
			"type": "object",
			"default": {
				"id": "",
				"src": ""
			}
		}
	}
}
