{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/repeater-items",
	"title": "Repetidor",
	"description": "Este componente cria uma repetição de outros componentes",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-repeater-items"],
	"render": "file:./render.php",
	"attributes": {
		"bgColor": {
			"type": "string",
			"default": "transparent"
		},
		"variant": {
			"type": "string",
			"default": "podium-item"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"bgColor": "transparent",
			"variant": "button"
		}
	}
