{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/hero-journey",
	"title": "Hero Journey",
	"description": "Primeira seção da página jornada senna, com duas variantes",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": ["file:./style.scss", "style-hero-journey"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"smallTitle": {
			"type": "boolean",
			"default": false
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"bgMob": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/placeholder360x684.jpeg",
				"alt": "background image"
			}
		},
		"bgDesk": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/placeholder1920x1080.jpeg",
				"alt": "background image"
			}
		},
		"isTextAux": {
			"type": "boolean",
			"default": false
		},
		"label": {
			"type": "string",
			"default": "Fórmula label"
		},
		"title": {
			"type": "string",
			"default": "Insira o <br>texto aqui</senna>"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}
