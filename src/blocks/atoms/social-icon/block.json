{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/social-icon",
	"title": "Social-Icon",
	"description": "Icon with social link",
	"category": "atoms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-social-icon"],
	"render": "file:./render.php",
	"attributes": {
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"variant": {
			"type": "string",
			"default": "default"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"icon": {
			"type": "object",
			"default": {
				"id": "",
				"src": ""
			}
		},
		"link":{
			"type": "string",
			"default": "#"
		}

	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"bgColor": "#ffffff",
			"variant": "default"
		}
	}
}
