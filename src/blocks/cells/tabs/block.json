{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/tabs",
	"title": "Abas (Tabs)",
	"description": "Componente para criação de abas",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"viewScript": "file:./view.js",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-tabs"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "transparent"
		},
		"itemsLength": {
			"type": "integer",
			"default": 0
		},
		"allowed_blocks": {
			"type": "array",
			"default": []
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"autoHeight": {
			"type": "boolean",
			"default": true
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "transparent"
		}
	}
}
