{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/collabs-hero",
	"title": "Hero - Collabs",
	"description": "A section with background image and 2 bottom logos senna and partner",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-collabs-hero"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"bgMob": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/placeholder360x684.jpeg",
				"alt": "background image"
			}
		},
		"bgDesk": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/placeholder1920x1080.jpeg",
				"alt": "background image"
			}
		},
		"sennaLogo": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/collabs/selo_bandeira.jpeg",
				"alt": "senna logo"
			}
		},
		"partnerLogo": {
			"type": "object",
			"default": {
				"src": "",
				"placeholder": "/wp-content/themes/Template-blocks/images/placeholder165x94.jpeg",
				"alt": "partner logo"
			}
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}
