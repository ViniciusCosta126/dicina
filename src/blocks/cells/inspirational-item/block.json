{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/inspirational-item",
	"title": "Inspirational-Item",
	"description": "Inspirational item",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": ["file:./style.scss", "style-inspirational-item"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"title": {
			"type": "string",
            "default": "ARETHA DUARTE"
		},
		"titleSenna": {
			"type": "string",
            "default": "BUSQUE SEU ÁPICE"
		},
		"profession": {
			"type": "string",
			"default": "Alpinista"
		},
		"phrase":{
			"type": "string",
			"default": "“Ousei escalar o Monte Everest, e fui a primeira mulher negra e periférica a fazer isso.”"
		},
		"instagram":{
			"type": "string",
			"default": "@aretha_duarte"
		},
		"video1Id":{
			"type": "string",
			"default": "hY1pT8kr9xQ"
		},
		"video2Id":{
			"type": "string",
			"default": "hY1pT8kr9xQ"
		},
		"video1Caption":{
			"type": "string",
			"default": "Verdade de Aretha"
		},
		"video2Caption":{
			"type": "string",
			"default": "Making of"
		},
		"videoThumb1": {
			"type": "object",
			"default": {
				"src": "/wp-content/themes/Template-blocks/images/bg-inspiring.jpg",
				"alt": "Aretha Duarte"
			}
		},
		"videoThumb2": {
			"type": "object",
			"default": {
				"src": "/wp-content/themes/Template-blocks/images/bg-inspiring.jpg",
				"alt": "Aretha Duarte"
			}
		},
		"bgImage": {
			"type": "object",
			"default": {
				"src": "/wp-content/themes/Template-blocks/images/bg-inspiring.jpg",
				"alt": "Aretha Duarte"
			}
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}
