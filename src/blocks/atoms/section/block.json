{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/section",
	"title": "Section",
	"description": "Componente para criação de seções",
	"category": "atoms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-section"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgImage": {
			"type": "object",
			"default": {
				"id": "",
				"src": ""
			}
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#002753"
		}
	}
}
