{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/overlay-line",
	"title": "Linha com Overlay",
	"description": "Line with a color overlay",
	"category": "atoms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": [
		"file:./style.scss",
		"style-overlay-line"
	],
	"render": "file:./render.php",
	"attributes": {
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"variant": {
			"type": "string",
			"default": "default"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"heightM": {
			"type": "integer",
			"default": 8
		},
		"heightT": {
			"type": "integer",
			"default": 8
		},
		"heightD": {
			"type": "integer",
			"default": 15
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"bgColor": "#002753",
			"variant": "default"
		}
	}
}