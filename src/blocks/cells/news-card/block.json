{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/news-card",
	"title": "Carde de nóticia",
	"description": "",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": [
		"file:./style.scss",
		"style-news-card"
	],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"cardimg": {
			"type": "object",
			"default": {
				"id": "",
				"src": "https://placeholder.co/550x774",
				"alt": "icone"
			}
		},
		"title": {
			"type": "string",
			"default": "Título"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}