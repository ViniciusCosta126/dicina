		"bgImg": {
			"type": "object",
			"default": {
				"id": "",
				"src": ""
			}
		},
		"firstImg": {
			"type": "object",
			"default": {
				"id": "",
				"src": "https://placeholder.co/75x75",
				"alt": "icone"
			}
		},
		"secondImg": {
			"type": "object",
			"default": {
				"id": "",
				"src": "https://placeholder.co/75x75",
				"alt": "icone"
			}
		},
		"thirdImg": {
			"type": "object",
			"default": {
				"id": "",
				"src": "https://placeholder.co/75x75",
				"alt": "icone"
			}
		},
		"valueTime": {
			"type": "number",
			"default": 1
		},
		"valueRacing": {
			"type": "number",
			"default": 1
		},
		"valuePole": {
			"type": "number",
			"default": 1
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"firstContent": {
			"type": "string",
			"default": "Primeiro Lugar"
		},
		"secondContent": {
			"type": "string",
			"default": "Segundo Lugar"
		},
		"thirdContent": {
			"type": "string",
			"default": "Terceiro Lugar"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
