{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/card-personality",
	"title": "Card Galeria Legado",
	"description": "Componente para a galeria de depoimentos legado",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-card-personality"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"smallImage": {
			"type": "object",
			"default": {
				"type": "image/jpeg",
				"src": "https://placehold.co/120x120/black/white",
				"alt": "Imagem placeholder"
			}
		},
		"hasModal": {
			"type": "boolean",
			"default": false
		},
		"modalId": {
			"type": "string",
			"default": ""
		},
		"hasVideo": {
			"type": "boolean",
			"default": false
		},
		"videoId": {
			"type": "string",
			"default": ""
		},
		"titleCard": {
			"type": "string",
			"default": ""
		},
		"indexId": {
			"type": "integer",
			"default": 1
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default"
		}
	}
}
