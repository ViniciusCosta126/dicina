{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/banner-imprensa",
	"title": "Imprensa - Banner Caterorias",
	"description": "Componente do banner da página de categorias de imprensa",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-banner-imprensa"],
	"render": "file:./render.php",
	"attributes": {
		"title": {
			"type": "string",
			"default": "Título"
		},
		"subtitle": {
			"type": "string",
			"default": "Subtitulo"
		},
		"hasSubtitle": {
			"type": "boolean",
			"default": true
		},
		"imgMobile": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/banner-imprensa-mobile.jpg"
			}
		},
		"imgDesk": {
			"type": "object",
			"default": {
				"id": "",
				"src": "/wp-content/themes/Template-blocks/images/banner-imprensa-desk.jpg"
			}
		}
	}
}
