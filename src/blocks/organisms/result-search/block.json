{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/result-search",
	"title": "Result-Search",
	"description": "Resultado das buscas",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-result-search"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"textSuccess": {
			"type": "string",
			"default": "Você buscou por:"
		},
		"textFail": {
			"type": "string",
			"default": "Não encontramos o que você procura"
		},
		"failLinkSuggestion":{
			"type": "string",
			"default": "Gostaria de sugerir um tema? Mande <a href='/contato'>aqui</a> pra gente e tente fazer uma nova busca com outras palavras."
		},
		"subTextFail": {
			"type": "string",
			"default": "Não temos notícias sobre o assunto:"
		},
		"textPostListFail": {
			"type": "string",
			"default": "MAIS LIDAS"
		},
		"auxTextPostListFail": {
			"type": "string",
			"default": "AS NOTÍCIAS"
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}
