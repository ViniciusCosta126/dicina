{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/brand-carousel",
	"title": "Carrossel de marcas",
	"description": "Seção com carrossel de 3 items para apresentação de marcas",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"style": ["file:./style.scss", "style-brand-carousel"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"hasLogo": {
			"type": "boolean",
			"default": true
		},
		"firstItem": {
			"type": "string",
			"default": "dark"
		},
		"contentAlign": {
			"type": "string",
			"default": "left"
		}
	}
}
