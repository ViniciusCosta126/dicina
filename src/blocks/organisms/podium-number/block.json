{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/podium-number",
	"title": "Seção Pódio",
	"description": "Componente para seção de podio",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:index.jsx",
	"script": "file:index.jsx",
	"viewScript": "file:view.js",
	"style": ["file:./style.scss", "style-podium-number"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
