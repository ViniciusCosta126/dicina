{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/testimony-of-personalities",
	"title": "Depoimentos personalidades",
	"description": "Componente para seção de depoimentos das personalidades",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"viewScript": "file:view.js",
	"style": ["file:./style-index.css", "style-testimony-of-personalities"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		},
		"hasImage": {
			"type": "boolean",
			"default": true
		},
		"bgImageMobile": {
			"type": "object",
			"default": {
				"type": "image/jpeg",
				"src": "https://placehold.co/768x768",
				"alt": "Imagem placeholder"
			}
		},
		"bgImageDesk": {
			"type": "object",
			"default": {
				"type": "image/jpeg",
				"src": "https://placehold.co/720x890",
				"alt": "Imagem placeholder"
			}
		},
		"caroselLength": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#002753"
		}
	}
}
