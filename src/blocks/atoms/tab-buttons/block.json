{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/tab-buttons",
	"title": "Tab Buttons",
	"description": "Cabe√ßalho para abas",
	"category": "atoms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
