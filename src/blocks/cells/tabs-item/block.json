{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/tabs-item",
	"title": "Item de abas",
	"description": "Item de aba para o componente tabs",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-tabs-item"],
	"render": "file:./render.php",
	"attributes": {
		"id": {
			"type": "string",
			"default": "tab-item"
		},
		"variant": {
			"type": "string",
			"default": "default"
		},
		"isActive": {
			"type": "boolean",
			"default": false
		},
		"allowed_blocks": {
			"type": "array",
			"default": []
		},
		"bgColor": {
			"type": "string",
			"default": "transparent"
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "transparent"
		}
	}
}
