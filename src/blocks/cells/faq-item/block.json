{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/faq-item",
	"title": "Item do Faq",
	"description": "Este componente servira como um item da faq, contendo a pergunta e a resposta",
	"category": "cells",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": [
		"file:./style-index.css",
		"style-faq-item"
	],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#ffffff"
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#ffffff"
		}
	}
}