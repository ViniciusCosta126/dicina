{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "il/racing-years",
	"title": "Corridas do ano",
	"description": "Seção para listagem de corridas do ano",
	"category": "organisms",
	"version": "1.0.0",
	"editorScript": "file:./index.jsx",
	"script": "file:./index.jsx",
	"editorStyle": "file:./style-index.css",
	"style": ["file:./style-index.css", "style-racing-years"],
	"render": "file:./render.php",
	"attributes": {
		"variant": {
			"type": "string",
			"default": "default"
		},
		"bgColor": {
			"type": "string",
			"default": "#002753"
		},
		"bgColorDegrade": {
			"type": "string",
			"default": "linear-gradient(to left, #002753 0, transparent 100%)"
		},
		"bgImage": {
			"type": "object",
			"default": {
				"type": "image/jpeg",
				"src": "https://placehold.co/720x890",
				"alt": "Imagem placeholder",
				"height": 890,
				"width": 720
			}
		},
		"title": {
			"type": "string",
			"default": "Corridas do ano"
		},
		"titleBreakeLine": {
			"type": "boolean",
			"default": false
		},
		"marginM": {
			"type": "integer",
			"default": 0
		},
		"marginD": {
			"type": "integer",
			"default": 0
		}
	},
	"example": {
		"attributes": {
			"preview": true,
			"blockOnly": true,
			"variant": "default",
			"bgColor": "#002753",
			"bgColorDegrade": "linear-gradient(to left, #002753 0, transparent 100%)"
		}
	}
}
